{
  "name": "express_ts_fullstack_template",
  "version": "1.2.0",
  "description": "Express Boilerplate Template Example with Typescript",
  "main": "index.js",
  "scripts": {
    "start": "node -r module-alias/register ./build",
    "build": "ts-node build.ts",
    "build:scss": "node-sass --recursive --output-style compressed ./public/scss/ -o ./public/dist/css",
    "dev": "SET DEBUG=express-ts-fullstack-template:* & nodemon",
    "dev:scss:watch": "npm run build:scss && node-sass --recursive --output-style compressed -w ./public/scss/ -o ./public/dist/css/",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "_moduleAliases": {
    "@routes": "build/routes",
    "@services": "build/services",
    "@models": "build/models",
    "@utilities": "build/utilities",
    "@public": "public",
    "@middlewares": "build/middlewares",
    "@constants": "build/constants",
    "@custom-types": "build/custom-types"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:@typescript-eslint/recommended-requiring-type-checking"
    ],
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "rules": {
      "max-len": [
        "error",
        {
          "code": 100
        }
      ],
      "no-console": 1,
      "no-extra-boolean-cast": 0,
      "@typescript-eslint/restrict-plus-operands": 0,
      "@typescript-eslint/explicit-module-boundary-types": 0,
      "@typescript-eslint/no-explicit-any": 0,
      "@typescript-eslint/no-floating-promises": 0,
      "@typescript-eslint/no-unsafe-member-access": 0,
      "@typescript-eslint/no-unsafe-assignment": 0,
      "@typescript-eslint/no-misused-promises": 0,
      "@typescript-eslint/no-unsafe-argument": 0,
      "@typescript-eslint/no-unsafe-call": 0
    }
  },
  "eslintIgnore": [
    "public/",
    "build.ts"
  ],
  "dependencies": {
    "@types/debug": "^4.1.7",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "debug": "^4.3.3",
    "dotenv": "^10.0.0",
    "ejs": "^3.1.6",
    "express": "^4.17.2",
    "express-validator": "^6.14.0",
    "fs-extra": "^10.0.0",
    "helmet": "^5.0.1",
    "http-errors": "^2.0.0",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "multer": "^1.4.4",
    "mysql2": "^2.3.3",
    "rotating-file-stream": "^3.0.2",
    "swagger-ui-express": "^4.3.0",
    "typeorm": "^0.2.41",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^9.0.13",
    "@types/http-errors": "^1.8.1",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.8",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "eslint": "^8.6.0",
    "morgan": "^1.10.0",
    "node-sass": "^7.0.1",
    "nodemon": "^2.0.15",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.4"
  }
}