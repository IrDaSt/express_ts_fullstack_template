{
  "name": "express_ts_fullstack_template",
  "version": "1.8.3",
  "description": "Express Boilerplate Template Example with Typescript",
  "scripts": {
    "start": "node ./build/index.js",
    "build": "ts-node build.ts",
    "build:scss": "sass --style=compressed --error-css --stop-on-error --color --trace ./public/scss/:./public/dist/scss-css",
    "build:docker": "docker build . -t danielwj00/express_ts_fullstack_template:1.8.3",
    "dev": "SET DEBUG=express-ts-fullstack-template:* & nodemon",
    "dev:scss:watch": "npm run build:scss && sass --style=compressed --error-css --update --watch --poll --stop-on-error --color --trace ./public/scss/:./public/dist/scss-css",
    "test": "npm run build && mocha --exit",
    "deploy:helm:install": "helm install express-ts-fullstack-template .kube_helm/",
    "deploy:helm:upgrade": "helm upgrade express-ts-fullstack-template .kube_helm/",
    "deploy:helm:uninstall": "helm uninstall express-ts-fullstack-template",
    "deploy:helm:run": "kubectl port-forward services/express-ts-fullstack-template 5000:5000"
  },
  "keywords": [],
  "author": "IrDaSt",
  "license": "ISC",
  "_moduleAliases": {
    "@app": "build/app.js",
    "@routes": "build/routes",
    "@services": "build/services",
    "@models": "build/models",
    "@utilities": "build/utilities",
    "@middlewares": "build/middlewares",
    "@constants": "build/constants",
    "@custom-types": "build/custom-types",
    "@test": "build/test"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:@typescript-eslint/recommended-requiring-type-checking"
    ],
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "rules": {
      "max-len": [
        "error",
        {
          "code": 100
        }
      ],
      "no-console": 1,
      "no-extra-boolean-cast": 0,
      "@typescript-eslint/restrict-plus-operands": 0,
      "@typescript-eslint/explicit-module-boundary-types": 0,
      "@typescript-eslint/no-explicit-any": 0,
      "@typescript-eslint/no-floating-promises": 0,
      "@typescript-eslint/no-unsafe-member-access": 0,
      "@typescript-eslint/no-unsafe-assignment": 0,
      "@typescript-eslint/no-misused-promises": 0,
      "@typescript-eslint/no-unsafe-argument": 0,
      "@typescript-eslint/no-unsafe-call": 0
    }
  },
  "eslintIgnore": [
    "public/",
    "build.ts"
  ],
  "dependencies": {
    "async": "^3.2.4",
    "chai": "^4.3.6",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "dotenv": "^16.0.1",
    "ejs": "^3.1.8",
    "express": "^4.18.1",
    "express-validator": "^7.0.1",
    "fs-extra": "^11.1.1",
    "helmet": "^7.0.0",
    "http-errors": "^2.0.0",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^9.0.0",
    "mocha": "^10.0.0",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.3.3",
    "nodemailer": "^6.7.8",
    "redis": "^4.3.0",
    "sharp": "^0.32.1",
    "supertest": "^6.2.4",
    "swagger-ui-express": "^4.5.0",
    "typeorm": "^0.3.9",
    "uuid": "^9.0.0",
    "winston": "^3.8.1",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@types/async": "^3.2.13",
    "@types/chai": "^4.3.3",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^11.0.1",
    "@types/http-errors": "^2.0.1",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/mocha": "^10.0.1",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.2.5",
    "@types/nodemailer": "^6.4.4",
    "@types/sharp": "^0.32.0",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "concurrently": "^8.1.0",
    "eslint": "^8.15.0",
    "nodemon": "^2.0.16",
    "sass": "^1.51.0",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^5.1.3"
  }
}